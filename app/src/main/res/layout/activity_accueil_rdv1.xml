<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/TxtAccueil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginTop="54dp"
        android:text="votre porte feuille de practicien"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/RvPracticien"
        android:layout_width="335dp"
        android:layout_height="464dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="@+id/TxtAccueil"
        app:layout_constraintStart_toStartOf="@+id/TxtAccueil"
        app:layout_constraintTop_toBottomOf="@+id/TxtAccueil" />
</androidx.constraintlayout.widget.ConstraintLayout>